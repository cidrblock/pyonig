[1.948, "o", "\u001b[38;5;144m// Dashboard Application - Main JavaScript\u001b[0m\r\n\u001b[38;5;222m'use strict'\u001b[0m;\r\n\r\n\u001b[38;5;144m// Configuration\u001b[0m\r\n\u001b[38;5;204mconst\u001b[0m \u001b[38;5;231mCONFIG\u001b[0m \u001b[38;5;204m=\u001b[0m {\r\n    apiEndpoint: \u001b[38;5;222m'/api/v1'\u001b[0m,\r\n    refreshInterval: \u001b[38;5;147m30000\u001b[0m,\r\n    maxRetries: \u001b[38;5;147m3\u001b[0m,\r\n    timeout: \u001b[38;5;147m5000\u001b[0m\r\n};\r\n\r\n\u001b[38;5;144m// API Service\u001b[0m\r\n\u001b[38;5;204mclass\u001b[0m APIService {\r\n    \u001b[38;5;204mconstructor\u001b[0m(\u001b[38;5;231mbaseURL\u001b[0m) {\r\n        \u001b[38;5;231mthis\u001b[0m.\u001b[38;5;231mbaseURL\u001b[0m \u001b[38;5;204m=\u001b[0m \u001b[38;5;231mbaseURL\u001b[0m;\r\n        \u001b[38;5;231mthis\u001b[0m.\u001b[38;5;231mretryCount\u001b[0m \u001b[38;5;204m=\u001b[0m \u001b[38;5;147m0\u001b[0m;\r\n    }\r\n\r\n    \u001b[38;5;204masync\u001b[0m \u001b[38;5;149mfetchMetrics\u001b[0m() {\r\n        \u001b[38;5;204mtry\u001b[0m {\r\n            \u001b[38;5;204mconst\u001b[0m \u001b[38;5;231mresponse\u001b[0m \u001b[38;5;204m=\u001b[0m \u001b[38;5;204mawait\u001b[0m \u001b[38;5;149mfetch\u001b[0m(\u001b[38;5;222m`\u001b[0m\u001b[38;5;204m${\u001b[0m\u001b[38;5;231mthis.baseURL\u001b[0m\u001b[38;5;204m}\u001b[0m\u001b[38;5;222m/metrics`\u001b[0m, {\r\n                method: \u001b[38;5;222m'GET'\u001b[0m,\r\n                headers: {\r\n                    \u001b[38;5;222m'Content-Type'\u001b[0m: \u001b[38;5;222m'application/json'\u001b[0m,\r\n                    \u001b[38;5;222m'Authorization'\u001b[0m: \u001b[38;5;222m`Bearer \u001b[0m\u001b[38;5;204m${\u001b[0m\u001b[38;5;231mthis.\u001b[0m\u001b[38;5;149mgetToken\u001b[0m\u001b[38;5;231m()\u001b[0m\u001b[38;5;204m}\u001b[0m\u001b[38;5;222m`\u001b[0m\r\n"]
