[1.725, "o", "\u001b[38;5;102m#!/usr/bin/env python3\u001b[0m\r\n\u001b[38;5;73m\"\"\"\r\n\u001b[0m\r\n\u001b[38;5;73mSample Python Application - Using pyonig API\r\n\u001b[0m\r\n\u001b[38;5;73mDemonstrates syntax highlighting as a library\r\n\u001b[0m\r\n\u001b[38;5;73m\"\"\"\u001b[0m\r\n\u001b[38;5;142mfrom\u001b[0m pathlib \u001b[38;5;142mimport\u001b[0m Path\r\n\u001b[38;5;142mfrom\u001b[0m typing \u001b[38;5;142mimport\u001b[0m Optional, List, Dict, Any\r\n\u001b[38;5;142mimport\u001b[0m json\r\n\u001b[38;5;142mimport\u001b[0m sys\r\n\r\n\u001b[38;5;142mimport\u001b[0m pyonig\r\n\r\n\r\n\u001b[38;5;145mdef\u001b[0m \u001b[38;5;74mhighlight_file_example\u001b[0m():\r\n    \u001b[38;5;73m\"\"\"Example 1: Highlight a file with pyonig.\"\"\"\u001b[0m\r\n    \u001b[38;5;74mprint\u001b[0m(\u001b[38;5;73m\"=== Example 1: Highlighting Files ===\u001b[0m\u001b[38;5;166m\\n\u001b[0m\u001b[38;5;73m\"\u001b[0m)\r\n    \r\n    \u001b[38;5;102m# Highlight a JSON file\u001b[0m\r\n    result \u001b[38;5;142m=\u001b[0m pyonig.highlight_file(\r\n        \u001b[38;5;73m'config.json'\u001b[0m,\r\n        theme\u001b[38;5;142m=\u001b[0m\u001b[38;5;73m'monokai'\u001b[0m,\r\n        output\u001b[38;5;142m=\u001b[0m\u001b[38;5;73m'ansi'\u001b[0m\r\n    )\r\n    \u001b[38;5;74mprint\u001b[0m(result)\r\n    \r\n    \u001b[38;5;102m# Auto-detect language from filename\u001b[0m\r\n    result \u001b[38;5;142m=\u001b[0m pyonig.highlight_file(\u001b[38;5;73m'app.py'\u001b[0m)\r\n"]
