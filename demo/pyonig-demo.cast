[1.606, "o", "<!\u001b[38;5;168mDOCTYPE\u001b[0m \u001b[38;5;215mhtml\u001b[0m>\r\n<\u001b[38;5;168mhtml\u001b[0m \u001b[38;5;215mlang\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143men\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n<\u001b[38;5;168mhead\u001b[0m>\r\n    <\u001b[38;5;168mmeta\u001b[0m \u001b[38;5;215mcharset\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mUTF-8\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n    <\u001b[38;5;168mmeta\u001b[0m \u001b[38;5;215mname\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mviewport\u001b[0m\u001b[38;5;181m\"\u001b[0m \u001b[38;5;215mcontent\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mwidth=device-width, initial-scale=1.0\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n    <\u001b[38;5;168mmeta\u001b[0m \u001b[38;5;215mname\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mdescription\u001b[0m\u001b[38;5;181m\"\u001b[0m \u001b[38;5;215mcontent\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mModern web application dashboard\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n    <\u001b[38;5;168mmeta\u001b[0m \u001b[38;5;215mname\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mauthor\u001b[0m\u001b[38;5;181m\"\u001b[0m \u001b[38;5;215mcontent\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mWeb Development Team\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n    <\u001b[38;5;168mtitle\u001b[0m>Dashboard - Analytics Portal</\u001b[38;5;168mtitle\u001b[0m>\r\n    <\u001b[38;5;168mlink\u001b[0m \u001b[38;5;215mrel\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mstylesheet\u001b[0m\u001b[38;5;181m\"\u001b[0m \u001b[38;5;215mhref\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mstyles.css\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n    <\u001b[38;5;168mlink\u001b[0m \u001b[38;5;215mrel\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143micon\u001b[0m\u001b[38;5;181m\"\u001b[0m \u001b[38;5;215mtype\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mimage/png\u001b[0m\u001b[38;5;181m\"\u001b[0m \u001b[38;5;215mhref\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mfavicon.png\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n</\u001b[38;5;168mhead\u001b[0m>\r\n<\u001b[38;5;168mbody\u001b[0m>\r\n    <\u001b[38;5;168mdiv\u001b[0m \u001b[38;5;215mclass\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mcontainer\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n        <\u001b[38;5;168mheader\u001b[0m \u001b[38;5;215mclass\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mmain-header\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n            <\u001b[38;5;168mh1\u001b[0m>Analytics Dashboard</\u001b[38;5;168mh1\u001b[0m>\r\n            <\u001b[38;5;168mp\u001b[0m \u001b[38;5;215mclass\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143msubtitle\u001b[0m\u001b[38;5;181m\"\u001b[0m>Real-time system monitoring and metrics</\u001b[38;5;168mp\u001b[0m>\r\n            <\u001b[38;5;168mnav\u001b[0m \u001b[38;5;215mclass\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mmain-nav\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n                <\u001b[38;5;168mul\u001b[0m>\r\n                    <\u001b[38;5;168mli\u001b[0m><\u001b[38;5;168ma\u001b[0m \u001b[38;5;215mhref\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143m#dashboard\u001b[0m\u001b[38;5;181m\"\u001b[0m>Dashboard</\u001b[38;5;168ma\u001b[0m></\u001b[38;5;168mli\u001b[0m>\r\n                    <\u001b[38;5;168mli\u001b[0m><\u001b[38;5;168ma\u001b[0m \u001b[38;5;215mhref\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143m#analytics\u001b[0m\u001b[38;5;181m\"\u001b[0m>Analytics</\u001b[38;5;168ma\u001b[0m></\u001b[38;5;168mli\u001b[0m>\r\n                    <\u001b[38;5;168mli\u001b[0m><\u001b[38;5;168ma\u001b[0m \u001b[38;5;215mhref\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143m#reports\u001b[0m\u001b[38;5;181m\"\u001b[0m>Reports</\u001b[38;5;168ma\u001b[0m></\u001b[38;5;168mli\u001b[0m>\r\n                    <\u001b[38;5;168mli\u001b[0m><\u001b[38;5;168ma\u001b[0m \u001b[38;5;215mhref\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143m#settings\u001b[0m\u001b[38;5;181m\"\u001b[0m>Settings</\u001b[38;5;168ma\u001b[0m></\u001b[38;5;168mli\u001b[0m>\r\n                </\u001b[38;5;168mul\u001b[0m>\r\n            </\u001b[38;5;168mnav\u001b[0m>\r\n        </\u001b[38;5;168mheader\u001b[0m>\r\n        \r\n        <\u001b[38;5;168mmain\u001b[0m \u001b[38;5;215mclass\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mcontent\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n            <\u001b[38;5;168msection\u001b[0m \u001b[38;5;215mclass\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mdashboard\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n                <\u001b[38;5;168mdiv\u001b[0m \u001b[38;5;215mclass\u001b[0m=\u001b[38;5;181m\"\u001b[0m\u001b[38;5;143mcard metric-card\u001b[0m\u001b[38;5;181m\"\u001b[0m>\r\n                    <\u001b[38;5;168mh2\u001b[0m>Total Users</\u001b[38;5;168mh2\u001b[0m>\r\n"]
