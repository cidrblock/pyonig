[1.589, "o", "\u001b[38;5;61m#!/usr/bin/env bash\u001b[0m\r\n\u001b[38;5;61m# Deployment Script - Production Infrastructure\u001b[0m\r\n\u001b[38;5;61m# Purpose: Deploy application to Kubernetes cluster with zero downtime\u001b[0m\r\n\r\n\u001b[38;5;140mset\u001b[0m -euo pipefail\r\n\r\n\u001b[38;5;61m# Configuration\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m CLUSTER_NAME=\u001b[38;5;71m\"production-us-east-1\"\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m NAMESPACE=\u001b[38;5;71m\"production\"\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m APP_NAME=\u001b[38;5;71m\"web-api\"\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m IMAGE_TAG=\u001b[38;5;71m\"\u001b[0m${1\u001b[38;5;67m:-\u001b[0mlatest}\u001b[38;5;71m\"\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m REPLICAS=\u001b[38;5;71m\"\u001b[0m${REPLICAS\u001b[38;5;67m:-\u001b[0m3}\u001b[38;5;71m\"\u001b[0m\r\n\r\n\u001b[38;5;61m# Colors for output\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m RED=\u001b[38;5;71m'\\033[0;31m'\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m GREEN=\u001b[38;5;71m'\\033[0;32m'\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m YELLOW=\u001b[38;5;71m'\\033[1;33m'\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m BLUE=\u001b[38;5;71m'\\033[0;34m'\u001b[0m\r\n\u001b[38;5;67mreadonly\u001b[0m NC=\u001b[38;5;71m'\\033[0m'\u001b[0m \u001b[38;5;61m# No Color\u001b[0m\r\n\r\n\u001b[38;5;61m# Logging functions\u001b[0m\r\n\u001b[38;5;187mlog_info\u001b[0m() {\r\n    \u001b[38;5;140mecho\u001b[0m -e \u001b[38;5;71m\"\u001b[0m${BLUE}\u001b[38;5;71m[INFO]\u001b[0m${NC}\u001b[38;5;71m \u001b[0m$*\u001b[38;5;71m\"\u001b[0m\r\n}\r\n\r\n\u001b[38;5;187mlog_success\u001b[0m() {\r\n    \u001b[38;5;140mecho\u001b[0m -e \u001b[38;5;71m\"\u001b[0m${GREEN}\u001b[38;5;71m[SUCCESS]\u001b[0m${NC}\u001b[38;5;71m \u001b[0m$*\u001b[38;5;71m\"\u001b[0m\r\n}\r\n\r\n\u001b[38;5;187mlog_warning\u001b[0m() {\r\n"]
